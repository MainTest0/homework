package ch01;

import java.io.FileWriter;
import java.io.IOException;

public class BufferedWriterExample {

	public static void main(String[] args) {

		// 글자를 만들어 두고 파일에다가 출력 하기
		// 출력 스트림 필요, 파일, 문자 단위
		try {
			new FileWriter("output1.txt");

		} catch (IOException e) {
			e.printStackTrace();
		}

	}// end of main

}// end of class
