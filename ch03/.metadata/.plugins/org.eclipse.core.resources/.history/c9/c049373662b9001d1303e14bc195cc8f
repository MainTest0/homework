package ch02;

import java.io.IOException;
import java.net.ServerSocket;
import java.net.Socket;

public class ServerTest {

// 서버 소켓 필요

	// 클라이언트로 연결받는 소켓
	ServerSocket serverSocket;
	// 실제 통신을 담당할 소켓 필요
	Socket socket;

	public ServerTest() {

	}

	private void initData() {
		try {
			serverSocket = new ServerSocket(10000);
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
