package ch04;

import java.io.FileWriter;
import java.io.IOException;

public class FileWriterTest1 {

	public static void main(String[] args) {

		FileWriter fw = null;
		try {
			fw = new FileWriter("writer1.txt");
		} catch (IOException e1) {
			e1.printStackTrace();
		}
		
		try {
			fw.write('H');		// 문자 하나만 출력 대상은 FIle
		} catch (IOException e) {
			e.printStackTrace();
		} finally {
			fw.close();
		}
		
	}// end of main

}// end of class
